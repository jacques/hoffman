{
    "name": "jacques/hoffman",
    "description": "Like Allen but with Slim 3",
    "require": {
        "illuminate/cache": "^9.52",
        "illuminate/database": "^9.52",
        "illuminate/events": "^9.52",
        "illuminate/filesystem": "^9.52",
        "illuminate/pagination": "^9.52",
        "illuminate/redis": "^9.52",
        "league/flysystem": "^3.15",
        "slim/slim": "^3.12",
        "vlucas/valitron": "^1.4"
    },
    "require-dev": {
        "brick/varexporter": "^0.3.8",
        "ergebnis/composer-normalize": "^2.31",
        "friendsofphp/php-cs-fixer": "^3.21",
        "phpunit/php-invoker": "^3.1",
        "phpunit/phpunit": "^9.6",
        "rector/rector": "^0.17.2",
        "vimeo/psalm": "^5.13"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "autoload": {
        "psr-4": {
            "Jacques\\Hoffman\\": "src/"
        }
    }
}
